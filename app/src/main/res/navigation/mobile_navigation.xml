<?xml version="1.0" encoding="utf-8"?>
<navigation
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/mobile_navigation"
        app:startDestination="@id/futureListWeather">

    <fragment
            android:id="@+id/futureListWeather"
            android:name="com.jaxfire.spacexinfo.ui.rocket_info.list.FutureListWeatherFragment"
            android:label="List"
            tools:layout="@layout/future_list_weather_fragment">
        <action
                android:id="@+id/action_futureListWeather_to_futureDetailWeather"
                app:destination="@id/futureDetailWeather"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment
            android:id="@+id/futureDetailWeather"
            android:name="com.jaxfire.spacexinfo.ui.rocket_info.detail.FutureDetailWeatherFragment"
            android:label="Detail"
            tools:layout="@layout/future_detail_weather_fragment"/>

</navigation>